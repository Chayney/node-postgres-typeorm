{
  "name": "node",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./dist/index.js",
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "dev": "nodemon --watch 'src/**/*' --exec 'ts-node' src/index.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm -- migration:generate src/database/migrations/TodoTable  --dataSource src/config/migrationDataSource.ts",
    "migration:run": "npm run typeorm -- migration:run --dataSource src/config/migrationDataSource.ts",
    "migration:revert": "npm run typeorm -- migration:revert --dataSource src/config/migrationDataSource.ts",
    "seed:run": "ts-node ./src/database/seeders/todo.seed.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "ts-node": "^10.9.2"
  }
}
